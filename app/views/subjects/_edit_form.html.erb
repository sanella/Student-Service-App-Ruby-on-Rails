          <%= form_for(@subject) do |f| %>
              <div class="form-group">
                <%= f.label :name %>
                <%= f.text_field :name, class: 'form-control' %>
              </div>
              <div class="form-group">
                <%= f.label :etcs %>
                <%= f.text_field :etcs, class: 'form-control' %>
              </div>
              <div class="form-group">
                <%= f.label :year %>
                <%= f.datetime_field :year, class: 'form-control' %>
              </div>
              <%= f.submit 'Edit professor', class: 'btn btn-primary btn-admin' %>
          <% end %>