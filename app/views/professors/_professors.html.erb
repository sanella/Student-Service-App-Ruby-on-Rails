<table class='table'>
	<thead>
		<tr>
			<td>Title</td>
			<td>First name</td>
			<td>Last name</td>
			<td></td>
		</tr>
	</thead>
	<tbody>
		<% @professors.each do |p| %>
		<tr>
			<td><%= p.title%></td>
			<td><%= p.first_name %></td>
			<td><%= p.last_name %></td>
			<td><%= link_to "Show", professor_path(p) %></td>
		</tr>
		<% end %>
	</tbody>
</table>
			